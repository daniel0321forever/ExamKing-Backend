{
    "builds": [
        {
            "src": "testing_game/asgi.py",
            "use": "@vercel/python",
            "config": { 
                "maxLambdaSize": "15mb", 
                "runtime": "Python3.11" 
            }
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "."
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "testing_game/asgi.py"
        }
    ]
}